<!--
    xml与HTML都有一个父亲——SGML（标准通用标记语言）
    HTML主要做页面展示，语法比较松散，随意
    xml主要做数据存储与数据描述，所以语法相当严格
-->

<students>
    <student sno="001">
        <sname>张三</sname>
        <sex>男</sex>
    </student>
    <student sno="002">
        <sname>李四</sname>
        <sex>男</sex>
    </student>
    <student sno="003">
        <sname>王五</sname>
        <sex>男</sex>
    </student>
</students>
